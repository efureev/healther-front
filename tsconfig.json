{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "sourceMap": true,
    "strict": true,
    "noUnusedLocals": true,
    "jsx": "preserve",
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "types": [
      "axios",
      "node",
      "vite/client",
      "unplugin-icons/types/vue",
      "vite-plugin-pages/client",
      "vite-plugin-vue-layouts/client"
    ],
    "typeRoots": [
      "."
    ],
    "paths": {
      "@/*": [
        "src/*"
      ],
      "~/*": [
        "*"
      ],
      "#/*": [
        "types/*"
      ]
    },
  }
}
